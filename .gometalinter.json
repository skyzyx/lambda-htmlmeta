{
    "Aggregate": true,
    "Concurrency": 4,
    "Deadline": "2m",
    "Debug": false,
    "DisableAll": true,
    "DuplThreshold": 50,
    "Enable": [
        "deadcode",
        "dupl",
        "errcheck",
        "gas",
        "goconst",
        "gofmt",
        "goimports",
        "golint",
        "gotype",
        "gotypex",
        "ineffassign",
        "interfacer",
        "lll",
        "maligned",
        "megacheck",
        "misspell",
        "nakedret",
        "structcheck",
        "test",
        "testify",
        "unconvert",
        "unparam",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "EnableGC": true,
    "Exclude": ["^/.*", "vendor/.*", ".*_test.go$"],
    "LineLength": 120,
    "MinConfidence": 80,
    "MinConstLength": 3,
    "MinOccurrences": 3,
    "MisspellLocale": "US",
    "Skip": ["vendor"],
    "Sort": ["linter", "severity", "path", "line"],
    "Test": true,
    "Vendor": true,
    "VendoredLinters": true
}
